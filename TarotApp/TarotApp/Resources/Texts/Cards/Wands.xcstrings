    "ace_of_wands.reversed.meaning" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "When reversed, the match sputters. Inspiration feels blocked, false starts exhaust\nyou, or passion burns too quickly to light anything lasting. You may doubt your\ncreative worth or struggle to channel scattered energy."
          }
